from multiprocessing.dummy import Pool as ThreadPool
import urllib3

pool = ThreadPool(4)
urls = [‘https://www.python.org', 'https://www.yahoo.com’, ‘https://www.google.com', 'https://pythondiario.com’]

results = pool.map(urilib3.connectino_from_url, urls)
for re in results:
	print(re)
pool.close()
pool.join()